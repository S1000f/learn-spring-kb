<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper>
    <select id="selectAll" parameterType="ioc.injection.BoardE" resultType="ioc.injection.BoardE">
        select * from boarde
    </select>
    <select id="selectContent">
        select * from boarde
        where num = #{aaanum}
    </select>
    <select id="insertBoarde">
        insert into boarde
    </select>
</mapper>